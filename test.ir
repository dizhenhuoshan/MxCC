
func gcd $x $y {
%__entry__gcd:
    $vreg = rem $x $y
    $vreg_2 = seq $vreg 0
    br $vreg_2 %__branch__if_then %__branch__if_else

%__branch__if_else:
    $vreg_3 = rem $x $y
    $vreg_4 = call gcd $y $vreg_3 
    $returnReg = move $vreg_4
    jump %__mergeReturn__gcd

%__branch__if_then:
    $returnReg = move $y
    jump %__mergeReturn__gcd

%__mergeReturn__gcd:
    ret $returnReg

}

func main {
%__entry__main:
    call __globalVarInit 
    $vreg = call gcd 10 4 
    call printForInt $vreg 
    ret 0

}

func __globalVarInit {
%__entry____globalVarInit:
    ret 0

}

